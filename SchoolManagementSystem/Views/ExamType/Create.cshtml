@model SchoolManagementSystem.Models.ExamType

<h2>Create Exam Type</h2>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="ExamTypeId">Exam ID</label>
        <input asp-for="ExamTypeId" class="form-control" />
        <span asp-validation-for="ExamTypeId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label>Exam Type Name</label>
        <input asp-for="ExamTypeName" class="form-control" />
    </div>
    <div class="form-group">
        <label>Max Marks</label>
        <input asp-for="MaxMarks" class="form-control" />
    </div>
    <button type="submit" class="btn btn-success">Save</button>
</form>
